{
  "name": "webpack-ts",
  "version": "1.0.0",
  "description": "Webpack with TypeScript",
  "main": "index.js",
  "browserslist": [
    "last 2 versions",
    "not IE 11",
    "not op_mini all",
    "not dead"
  ],
  "scripts": {
    "start": "npm run build:dev && npm run dev-server",
    "build:dev": "webpack --config webpack.config.js --env development mode=development",
    "build:prod": "webpack --config webpack.config.js --env production mode=production",
    "dev-server": "webpack serve --config webpack.config.js --env development mode=development"
  },
  "author": "karihold",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-regenerator": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/runtime": "^7.21.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.30.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "node-sass": "^8.0.0",
    "postcss": "^8.4.21",
    "postcss-csso": "^6.0.1",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^8.3.1",
    "sass-loader": "^13.2.2",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "typescript": "^5.0.4"
  }
}
